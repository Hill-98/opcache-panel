{
    "name": "hill-98/opcache-panel",
    "description": "PHP opcache panel",
    "type": "project",
    "license": "MIT",
    "version": "1.5.1",
    "homepage": "https://github.com/Hill-98/opcache-panel",
    "authors": [
        {
            "name": "Zhong Lufan",
            "email": "lufanzhong@gmail.com"
        }
    ],
    "scripts": {
        "serve": "php -S 127.0.0.1:8098"
    },
    "require": {
        "php": "^7.0 | ^8.0",
        "ext-zend-opcache": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.18.5"
    },
    "autoload": {
        "files": [
            "define.php"
        ],
        "psr-4": {
            "OpcachePanel\\": "OpcachePanel/"
        }
    }
}
